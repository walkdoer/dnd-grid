//###枚举类型
{
    //类型 enum/number/string
    type: "enum",
    //展现形式 ratio/dropmenu/list
    display: "ratio",
    //字段显示名称
    text: "PV/UV",
    //字段名称，对应于数据库字段
    name: "indicator",

    items: [{
        text: "PV",
        name: "pv",
        value: "p"
    }, {
        text: "UV",
        name: "uv",
        value: "u"
    }]
}

//return example
{
    indicator: "u"
}


//### 数字类型
{

    type: "number",
    text: "Top N",
    name: "topN",
    default: 5
}
// return example
{
    topN: 5
}


//### 字符串类型
{

    type: "string",
    text: "姓名",
    name: "yourname",
    default: 'test name'
}
// return example
{
    yourname: 'piccasa'
}


// 组合类型

{
    name: 'filter',
    text: '过滤条件',
    type: 'combined',
    display: 'list',
    default: {
        pv_uv: {
            operator: 'equal',
            value: '10'
        },
        example: {
            operator: 'lt',
            value: 100
        }
    },
    items: [{
        name: 'operator',
        type: 'enum',
        display: 'dropmenu',
        items: [{
            text: '大于',
            value: 'bt'
        }, {
            text: '等于',
            value: 'equal'
        }]
    }, {
        type: 'number',
        name: 'value',
        default: 0
    }]
}


